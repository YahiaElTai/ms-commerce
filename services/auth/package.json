{
  "name": "cart",
  "version": "1.0.0",
  "scripts": {
    "start:dev": "dotenv -e .env.development nodemon src/index.ts",
    "start": "node build/index.js",
    "test": "dotenv -e .env.test jest --watchAll --no-cache ./src",
    "test:ci": "dotenv -e .env.test jest ./src",
    "build": "rimraf build && tsc",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=src/prisma/schema.prisma"
  },
  "dependencies": {
    "@ms-commerce/common": "^1.0.6",
    "@prisma/client": "^4.7.1",
    "cookie-parser": "^1.4.6",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
