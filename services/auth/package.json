{
  "name": "cart",
  "version": "1.0.0",
  "scripts": {
    "dev": "dotenv -e .env.development -- nodemon src/index.ts",
    "start": "node build/index.js",
    "jest:watch": "dotenv -e .env.test -- jest --watchAll --no-cache ./src",
    "jest:ci": "dotenv -e .env.test -- jest ./src",
    "build": "rimraf build && tsc",
    "typecheck": "tsc --version && tsc --noEmit",
    "prisma:generate:dev": "dotenv -e .env.development -- prisma generate --schema=src/prisma/schema.prisma",
    "prisma:migrate:dev:dev": "dotenv -e .env.development -- prisma migrate dev --schema=src/prisma/schema.prisma",
    "prisma:migrate:dev:test": "dotenv -e .env.test -- prisma migrate dev --schema=src/prisma/schema.prisma",
    "prisma:migrate:deploy:dev": "dotenv -e .env.development -- npx prisma migrate deploy --schema=src/prisma/schema.prisma",
    "prisma:migrate:deploy:test": "dotenv -e .env.test -- npx prisma migrate deploy --schema=src/prisma/schema.prisma",
    "test:setup": "docker-compose up -d && npm run prisma:migrate:deploy:test",
    "test:watch": "npm run test:setup && npm run jest:watch",
    "test:ci": "npm run test:setup && npm run jest:ci"
  },
  "dependencies": {
    "@ms-commerce/common": "^1.0.8",
    "@prisma/client": "^4.7.1",
    "cookie-parser": "^1.4.6",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
