{
  "name": "auth",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "start": "node build/index.js",
    "test": "jest --watchAll --no-cache ./src",
    "test:ci": "jest ./src",
    "build": "rimraf build && tsc",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:dev": "prisma migrate dev --schema=src/prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema=src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=src/prisma/schema.prisma",
    "lint": "eslint --ext .ts .",
    "format": "prettier --write '**/*.ts'"
  },
  "dependencies": {
    "@ms-commerce/common": "^1.0.9",
    "@prisma/client": "^4.7.1",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
