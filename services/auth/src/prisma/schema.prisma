generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  v        Int    @default(0) @map("__v") @ignore
  email    String @unique(map: "email_1")
  password String

  @@map("users")
}
